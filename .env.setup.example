# ============================================
# Setup Wizard Configuration
# ============================================

# Setup Access Token (auto-generated on first run)
# Leave empty to generate automatically
# Set to disable wizard after successful setup
SETUP_TOKEN=

# Automatically disable wizard after successful deployment
# Set to "false" to allow re-running setup (NOT RECOMMENDED for production)
DISABLE_AFTER_SETUP=true

# Port for setup wizard (internal)
SETUP_PORT=3000

# Workspace directory (where .env files are located)
WORKSPACE_DIR=/workspace
